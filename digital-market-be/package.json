{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch --type-check",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:e2e": "vitest --config vitest.config-e2e.ts",
    "typeorm:cli": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm:cli -- -d src/libs/typeorm.config.ts migration:generate src/migrations/migrations",
    "migration:up": "npm run typeorm:cli migration:run -- -d src/libs/typeorm.config.ts",
    "migration:down": "npm run typeorm:cli -- -d src/libs/typeorm.config.ts migration:revert",
    "typechain:gen": "typechain --target ethers-v6 --out-dir src/contracts src/abis/**/*.json"
  },
  "dependencies": {
    "@dataui/crud": "^5.3.4",
    "@dataui/crud-typeorm": "^5.3.4",
    "@google/generative-ai": "^0.21.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.6",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "ethers": "^6.13.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "minio": "^8.0.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nestjs-minio-client": "^2.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
